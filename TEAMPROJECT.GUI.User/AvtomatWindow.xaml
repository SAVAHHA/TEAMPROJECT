        <Button Name = "MainMenuButton" Grid.Row="2" Grid.Column="0" Margin="128,46.333,128.667,36.667" RenderTransformOrigin="0.5,0.5" Click="MainMenuButton_Click"> В главное меню
        </Button>
        <Button Name = "ExitButton" Grid.Row="2" Grid.Column="1" Margin="84.333,52.333,141.333,36.667" RenderTransformOrigin="0.5,0.5" Click="ExitButton_Click" >Выйти
        </Button>
    </Grid>
</Window>
